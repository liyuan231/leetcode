<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="designPattern.mid.anotherTry.dao.IUserDao">


    <select id="queryUserByUsername" parameterType="java.lang.String" resultType="designPattern.mid.anotherTry.pojo.Users">
        select *
        from users
        where username = #{id};
    </select>
</mapper>